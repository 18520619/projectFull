<div class="form-floating mb-3">
    <input type="text" value="<%= product.name %>" class="form-control" id="floatingInput"  name="name" placeholder="Product Name">
    <label for="floatingInput">Product Name</label>
  </div>
  <div class="form-floating mb-3 ">
    <input type="text" value="<%= product.info %>" class="form-control" id="floatingPassword" name="info" placeholder="Product Information">
    <label for="floatingInput">Product Information</label>
  </div>
  <div class="form-floating mb-3">
    <input type="text" value="<%= product.price %>" class="form-control" id="floatingInput" name="price" placeholder="Price ">
    <label for="floatingInput">Price</label>
  </div>
  <div class="form-floating mb-3">
    <input type="text" value="<%= product.quantity %>" class="form-control" id="floatingPassword" name="quantity" placeholder="Product Quantity">
    <label for="floatingInput">Product Quantity</label>
  </div>
  <div class="form-floating mb-3">
    <select name="category" class="form-select" id="floatingSelect" aria-label="Floating label select example">
      <option selected>Category</option>
     <% 
        categories.map(cate=>{%>
                <option value="<%= cate._id %>"><%= cate.name %></option>
            <%})
     %>
    </select>
    
  </div>
  <div class="form-floating mb-3 w-50">
    <input type="submit" class="form-control btn btn-primary" id="sub" value="save"  placeholder="Save">
   
  </div>